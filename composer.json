{
    "name": "factoryplus/manager",
    "type": "project",
    "description": "Central management application for Factory+",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-simplexml": "^8.0",
        "amrc-factoryplus/service-client": "^1.0.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^3.0",
        "fzaninotto/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.5",
        "http-interop/http-factory-guzzle": "^1.2",
        "itsgoingd/clockwork": "^5.0",
        "laravel/fortify": "^1.14",
        "laravel/framework": "^9.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/octane": "^1.0.0",
        "laravel/passport": "^11.9",
        "laravel/scout": "^9.2",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "meilisearch/meilisearch-php": "^v0.26.0",
        "opis/json-schema": "^2.3",
        "pusher/pusher-php-server": "^5.0",
        "sentry/sentry-laravel": "^2.13",
        "shalvah/laravel-jsend": "^2.3",
        "spatie/laravel-query-builder": "^5.0.3",
        "spatie/laravel-ray": "^1.29",
        "spatie/test-time": "^1.2",
        "staudenmeir/eloquent-eager-limit": "^1.0",
        "stevebauman/purify": "^4.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-xmlreader": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.4",
        "brianium/paratest": "^v6.3.1",
        "johnkary/phpunit-speedtrap": "^3.3",
        "laravel/sail": "^1.10",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ignition": "^1.0",
        "tightenco/duster": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Support/Helpers/action_response_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
