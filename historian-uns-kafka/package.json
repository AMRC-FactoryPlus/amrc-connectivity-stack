    "clean": "tsc --build --clean",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@amrc-factoryplus/service-client": "^1.3.6",
    "async": "^3.2.4",
    "gssapi.js": "^2.0.1",
    "kafkajs": "^2.2.4",
    "long": "^5.2.3",
    "mqtt": "^5.9.1",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0"
  }
}
