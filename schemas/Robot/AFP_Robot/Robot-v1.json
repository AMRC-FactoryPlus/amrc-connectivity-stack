{
  "$id": "urn:uuid:240ae2d4-2010-476d-a9ce-f03a2c419adc",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AFP Robot",
  "type": "object",
  "properties": {
    "Schema_UUID": {
      "const": "240ae2d4-2010-476d-a9ce-f03a2c419adc"
    },
    "Instance_UUID": {
      "description": "The unique identifier for this object. (A UUID specified by RFC4122).",
      "type": "string",
      "format": "uuid"
    },
    "Base_Robot": {
      "$ref": "urn:uuid:6012580d-fd0a-442c-8d71-7f31f7155633"
    },
    "Machine_State": {
      "allOf": [
        {
          "$ref": "urn:uuid:b16275f1-e443-4c41-a482-fcbdfbd20769"
        },
        {
          "properties": {
            "Sparkplug_Type": {
              "enum": [
                "Int32LE",
                "Int32BE"
              ]
            },
            "Documentation": {
              "default": "An integer that represents the machine being in one of several possible states."
            }
          }
        }
      ]
    },
    "Current_Course_Duration": {
      "allOf": [
        {
          "$ref": "urn:uuid:b16275f1-e443-4c41-a482-fcbdfbd20769"
        },
        {
          "properties": {
            "Sparkplug_Type": {
              "enum": [
                "FloatLE",
                "FloatBE"
              ]
            },
            "Documentation": {
              "default": "A value representing the time in seconds that the current course has been running"
            }
          }
        }
      ]
    },
    "Heads": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z0-9_]*$": {
          "$ref": "urn:uuid:d50b78ea-83c5-4a5d-8514-68fd882eaa0b"
        }
      }
    }
  },
  "required": [
    "Schema_UUID",
    "Instance_UUID"
  ]
}